{
  "name": "leostar-ui",
  "version": "0.0.3",
  "type": "module",
  "license": "MIT",
  "scripts": {
    "dev": "vite",
    "start": "esno src/index.ts",
    "build": "vue-tsc && vite build",
    "preview": "vite preview",
    "test": "vitest",
    "test:ui": "vitest --ui",
    "lint": "eslint src/directive/**/*.ts",
    "fix": "eslint src/**/*.*  --fix",
    "typecheck": "tsc --noEmit",
    "docs:dev": "vitepress dev docs",
    "docs:build": "vitepress build docs",
    "docs:preview": "vitepress preview docs",
    "release": "bumpp --commit --push --tag && npm publish",
    "up": "taze major -I"
  },
  "homepage": "https://github.com/LeoStar-UI/leostar-ui#readme",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/LeoStar-UI/leostar-ui.git",
    "directory": "packages/leostar-ui"
  },
  "bugs": {
    "url": "https://github.com/LeoStar-UI/leostar-ui/issues"
  },
  "keywords": [
    "leostar-ui",
    "unocss",
    "vueuse"
  ],
  "sideEffects": false,
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/leostar-ui.js",
      "require": "./dist/leostar-ui.umd.cjs"
    }
  },
  "main": "./dist/leostar-ui.umd.cjs",
  "module": "./dist/leostar-ui.js",
  "types": "./dist/types/leostar-ui/src/index.d.ts",
  "files": [
    "dist",
    "volar.d.ts"
  ],
  "publishConfig": {
    "access": "public",
    "registry": "https://registry.npmjs.org/"
  },
  "dependencies": {
    "vue": "^3.4.19"
  },
  "devDependencies": {
    "@antfu/eslint-config": "^2.6.4",
    "@types/node": "^20.11.24",
    "@vitejs/plugin-vue": "^5.0.4",
    "@vitejs/plugin-vue-jsx": "^3.1.0",
    "@vitest/ui": "^1.3.1",
    "@vueuse/core": "^10.9.0",
    "bumpp": "^9.3.0",
    "eslint": "^8.57.0",
    "eslint-plugin-format": "^0.1.0",
    "eslint-ts-patch": "8.56.0-0",
    "esno": "^4.0.0",
    "lint-staged": "^15.2.2",
    "sass": "^1.71.1",
    "simple-git-hooks": "^2.9.0",
    "taze": "^0.13.3",
    "typescript": "^5.2.2",
    "unocss": "^0.58.5",
    "unplugin-auto-import": "^0.17.5",
    "unplugin-vue-components": "^0.26.0",
    "vite": "^5.1.4",
    "vite-plugin-dts": "^3.7.3",
    "vitepress": "1.0.0-rc.44",
    "vitest": "^1.3.1",
    "vue-tsc": "^1.8.27"
  }
}
